{
  "name": "tauri-plugin-automation-api",
  "version": "0.2.0",
  "description": "JavaScript API for tauri-plugin-automation - HTTP automation server for Tauri apps (v1 and v2)",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "build": "tsup src/index.ts --format cjs,esm --dts",
    "dev": "tsup src/index.ts --format cjs,esm --dts --watch"
  },
  "keywords": [
    "tauri",
    "tauri-v1",
    "tauri-v2",
    "automation",
    "testing",
    "webdriver"
  ],
  "author": "Corey Herrera",
  "license": "SEE LICENSE IN ../LICENSE",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dcherrera/tauri-plugin-automation.git"
  },
  "homepage": "https://github.com/dcherrera/tauri-plugin-automation#readme",
  "bugs": {
    "url": "https://github.com/dcherrera/tauri-plugin-automation/issues"
  },
  "peerDependencies": {
    "@tauri-apps/api": "^1.5.0 || ^2.0.0"
  },
  "devDependencies": {
    "tsup": "^8.0.0",
    "typescript": "^5.0.0"
  }
}
